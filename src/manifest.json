
{
   "action": {   
   },
   "content_scripts": [
   {
      "all_frames": true,
      "js": [ "contentScript.js" ],
      "matches": [ "https://www.montana.edu/*" ],
      "run_at": "document_idle"
   }],
   "background": {
      "service_worker": "background.js"
   },
   "permissions": ["contextMenus", "storage", "unlimitedStorage", "scripting"],
   "host_permissions": ["https://www.montana.edu/*"],
   "options_ui": {
      "page": "options.html",
      "open_in_tab": true
   },
   "content_security_policy": {
      "extension_page":"script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
   },
   "description": "The purpose of the Gray MSU extension is to unintrusively show the broken and dead links on Montana State University&#39;s numerous websites.",
   "host_permissions": [ "https://www.montana.edu/*" ],
   "icons": {
      "128": "logo/logo-128.png",
      "48": "logo/logo-48.png"
   },
   "manifest_version": 3,
   "name": "MSU Unlinked",
   "version": "0.10.2"
}
